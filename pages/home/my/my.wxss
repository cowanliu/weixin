  font-size: 32rpx;
  margin-bottom: 40rpx;
  border-radius: 10rpx;
}
.footer .xiangqing {
  border: 1rpx solid beige;
  height: 78rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.footer .xiangqing .left {
padding-left: 60rpx;
}
.footer .xiangqing .right {
